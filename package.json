{
  "name": "slack-mocker",
  "version": "1.2.5",
  "description": "Slack API mocker",
  "main": "index.js",
  "scripts": {
    "pretest": "standard index.js src/*.js src/**/*.js test/*.js test/**/*.js examples/**/*.js --fix",
    "test": "nyc _mocha --recursive ./test/",
    "examples": "SLACK_TOKEN='single-team-1' mocha --recursive ./examples/test/ ",
