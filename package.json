{
  "name": "slack-mock",
  "version": "1.0.0-beta.2",
  "description": "Slack API mocker",
  "main": "index.js",
  "scripts": {
    "pretest": "standard index.js src/*.js src/**/*.js test/*.js test/**/*.js examples/**/*.js --fix",
    "test": "istanbul cover --report lcov --dir ./coverage/ _mocha --recursive ./test/ --grep ./test/**/*.spec.js -- --colors --reporter spec",
    "examples": "SLACK_TOKEN='single-team-1' mocha --recursive ./examples/test/ ",
    "ci": "npm test && cat ./coverage/lcov.info | coveralls"
  },
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/colestrode/slack-mock.git"
  },
  "dependencies": {
    "express": "^4.14.0",
    "nock": "^9.0.2",
    "qs": "^6.3.0",
    "request": "^2.79.0",
    "winston": "^2.3.0",
    "ws": "^1.1.1"
  },
  "devDependencies": {
    "body-parser": "^1.15.2",
    "chai": "^3.5.0",
    "coveralls": "^2.11.15",
    "delay": "^1.3.1",
    "istanbul": "^0.4.5",
    "mocha": "^3.2.0",
    "proxyquire": "^1.7.10",
    "sinon": "^1.17.6",
    "sinon-chai": "^2.8.0",
    "standard": "^8.5.0"
  },
  "standard": {
    "globals": [
      "after",
      "afterEach",
      "before",
      "beforeEach",
      "describe",
      "it"
    ]
  }
}
